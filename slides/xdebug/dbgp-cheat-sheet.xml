<?xml version="1.0" encoding="ISO-8859-1"?>
<slide fontsize="6em">
	<title>DBGp Cheat Sheet</title>

	<blurb>Execution:</blurb>
	<blurb class="small_blurb">%run%</blurb>
	<blurb class="small_blurb">%step_into%</blurb>
	<blurb class="small_blurb">%step_over%</blurb>
	<blurb class="small_blurb">%step_out%</blurb>
	<break/>

	<blurb>Breakpoints:</blurb>
	<blurb class="small_blurb">%breakpoint_set *-t* [type] *-f* [filename] *-n* [lineno]%</blurb>
	<blurb class="small_blurb">% *-a* [classname] *-m* [functionname]%</blurb>
	<blurb class="small_blurb">~types: line, call, return, exception, conditional~</blurb>
	<break/>

	<blurb class="small_blurb">%breakpoint_list%</blurb>
	<blurb class="small_blurb">%breakpoint_get *-d* [breakpoint_id]%</blurb>
	<blurb class="small_blurb">%breakpoint_remove *-d* [breakpoint_id]%</blurb>
	<break/>

	<blurb>Working with stack:</blurb>
	<blurb class="small_blurb">%stack_depth%</blurb>
	<blurb class="small_blurb">%stack_get%</blurb>
	<blurb class="small_blurb">%context_names%</blurb>
	<blurb class="small_blurb">%context_get *-d* [depth] *-c* [context_id]%</blurb>
	<break/>

	<blurb>Working with variables:</blurb>
	<blurb class="small_blurb">%property_get *-d* [depth] *-n* [varname]%</blurb>
	<blurb class="small_blurb">%property_set *-d* [depth] *-n* [varname] *--* [uuencoded_data]%</blurb>
</slide>
